<%- include('../partials/header') %>

<table class="table table-striped table-hover table-light">
    <thead>
      <tr>
        <th scope="col">Story</th>
      </tr>
    </thead>
    <tbody>
      <% stories.forEach(function(story){ %>
        <th scope="row">
          <%= story.questionOne %>
        </th>
        <td><%= story.question %></td>
      </tr>
      <% }); %>
    </tbody>
  </table>






